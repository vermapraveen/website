@page "{catName?}"
@model Pkv.View.Pages.blogs.IndexModel
@inject Pkv.Common.IDebugInfoHelper debugInfoHelper

@{
    var traceRef = debugInfoHelper.Start($"Blogs/{@Model.BlogUniqueName}");
}
@await Component.InvokeAsync("BlogPage", new { blogUniqueName = Model.BlogUniqueName })

@{
    debugInfoHelper.End(traceRef);
    debugInfoHelper.ConsolePrintFormattedDebugInfo();
}
