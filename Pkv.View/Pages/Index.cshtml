@page "{handler?}"
@model IndexModel
@inject Pkv.Common.IDebugInfoHelper debugInfoHelper

@{
    var traceRef = debugInfoHelper.Start("IndexView");
}
@{
    ViewData["Title"] = "Praveen Verma";
}

@await Component.InvokeAsync("BlogPage", new { uniqueName = Model.UniqueName })


@{
    debugInfoHelper.End(traceRef);
    debugInfoHelper.ConsolePrintFormattedDebugInfo();
}
