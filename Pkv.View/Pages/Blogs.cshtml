@page
@model Pkv.View.Pages.BlogsModel
@inject Pkv.Common.IDebugInfoHelper debugInfoHelper

@{
    ViewData["Title"] = "Blogs of";
    var traceRef = debugInfoHelper.Start("BlogsView");
}

@await Component.InvokeAsync("BlogList", new { blogIntros = Model.Blogs })

@{
    debugInfoHelper.End(traceRef);
    debugInfoHelper.ConsolePrintFormattedDebugInfo();
}
